   "source": [
    "bag = []\n",
    "for i in range(10000):\n",
    "    prize = [\"羊\",\"羊\",\"車\"]\n",
    "    choose = random.choice(prize)\n",
    "    prize.remove(choose)\n",
    "    prize.remove(\"羊\")\n",
    "    bag.append(prize[0])\n",
    "print(\"換選另一個門得到車子的機率為{}\".format(bag.count(\"車\")/10000))"
   ]
